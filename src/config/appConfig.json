{
    "app": {
        "name": "Dynamic Frontend System",
        "version": "1.0.0"
    },
    "themes": {
        "light": {
            "primary": "#1E90FF",
            "secondary": "#FFFFFF",
            "textColor": "#333"
        },
        "dark": {
            "primary": "#0A0A0A",
            "secondary": "#1E1E1E",
            "textColor": "#EEE"
        }
    },
    "layouts": {
        "user": {
            "layout_id": 3,
            "theme": "light",
            "header": {
                "variant": "centered",
                "height": 60,
                "style": {
                    "backgroundColor": "#1E90FF",
                    "color": "#FFFFFF",
                    "padding": "0 20px",
                    "display": "flex",
                    "alignItems": "center",
                    "justifyContent": "space-between"
                },
                "show_header": true
            },
            "sider": {
                "variant": "pill",
                "width": 250,
                "height": "100vh",
                "style": {
                    "backgroundColor": "#F5F5F5",
                    "padding": "20px"
                },
                "items": [
                    {
                        "label": "Home",
                        "key": "home",
                        "route": "/home",
                        "additionalPaths": [],
                        "show_sider": true,
                        "children": [],
                        "content": "<div><h1>Home</h1></div>",
                        "header_items": []
                    },
                    {
                        "label": "Settings",
                        "key": "settings",
                        "show_sider": true,
                        "route": "/settings",
                        "additionalPaths": [
                            "/profile",
                            "/my-apps",
                            "/my-enrollments",
                            "/my-devices"
                        ],
                        "children": [],
                        "content": "<div><h1>Settings</h1></div>",
                        "header_items": [
                            {
                                "label": "Profile",
                                "key": "profile",
                                "route": "/profile",
                                "content": "<div><h1>Profile</h1></div>"
                            },
                            {
                                "label": "My Apps",
                                "key": "my-apps",
                                "route": "/my-apps",
                                "content": "<div><h1>My Apps</h1></div>"
                            },
                            {
                                "label": "My Enrollments",
                                "key": "my-enrollments",
                                "route": "/my-enrollments",
                                "content": "<div><h1>My Enrollments</h1></div>"
                            },
                            {
                                "label": "My Devices",
                                "key": "my-devices",
                                "route": "/my-devices",
                                "content": "<div><h1>My Devices</h1></div>"
                            }
                        ]
                    }
                ]
            }
        }
    }
}